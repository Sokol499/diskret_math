def sort_arcs(arcs):
    arc_list = [tuple(map(int, arc.split('-'))) for arc in arcs.split(', ')]
    sorted_arcs = sorted(arc_list, key=lambda x: (x[0], x[1]))
    sorted_arcs_str = [f"{a}-{b}" for a, b in sorted_arcs]
    return sorted_arcs_str

input_arcs = "1-3, 0-2, 1-2, 0-1, 5-6, 7-8, 2-3"
output = sort_arcs(input_arcs)
print(output)  

# Результат: ['0-1', '0-2', '1-2', '1-3', '2-3', '5-6', '7-8']
