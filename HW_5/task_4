from collections import defaultdict

ver = [0, 1, 2, 3, 4, 5, 6, 7, 8]
ed = [(0, 1), (0, 2), (0, 5), (0, 7), (1, 0), (1, 5), (2, 0), (2, 3), (2, 7), (3, 2), (3, 4), (4, 3), (5, 0), (5, 1), (6, 8), (7, 0), (7, 2), (8, 6)]

unique_edges = defaultdict(list)

for x, y in ed:
    if (x, y) not in unique_edges[x] and (y, x) not in unique_edges[x]:
        unique_edges[x].append((x, y))

for v in ver:
    print(f"{v}: {unique_edges[v]}")
